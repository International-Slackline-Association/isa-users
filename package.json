{
  "name": "isa-users",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "lint": "eslint .",
    "prettifyAll": "prettier --write '{src,test}/**/*.ts'",
    "checkTs": "tsc --noEmit",
    "package": "serverless package",
    "deploy": "serverless deploy --verbose",
    "deploy:api": "serverless deploy function -f api",
    "deploy:cognito": "serverless deploy function -f cognitoTrigger",
    "deploy:verification-api": "serverless deploy function -f verificationApi",
    "deploy:public-api": "serverless deploy function -f publicApi",
    "invoke:migrations": "serverless invoke local -f migrations",
    "scripts:create-user": "ts-node misc/scripts/create-user.ts",
    "scripts:listIncorrectItems": "ts-node misc/scripts/listIncorrectItems.ts",
    "scripts:remindUnverifiedUsers": "ts-node misc/scripts/remindUnverifiedUsers.ts",
    "scripts:listOrganizationMembers": "ts-node misc/scripts/listOrganizationMembers.ts"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "3.478.0",
    "@aws-sdk/client-cognito-identity-provider": "3.478.0",
    "@aws-sdk/client-dynamodb": "3.478.0",
    "@aws-sdk/client-ses": "3.478.0",
    "@aws-sdk/client-ssm": "3.478.0",
    "@aws-sdk/lib-dynamodb": "3.478.0",
    "@vendia/serverless-express": "4.12.6",
    "cors": "2.8.5",
    "dateformat": "5.0.3",
    "express": "4.18.2",
    "googleapis": "129.0.0",
    "jsonwebtoken": "9.0.2",
    "lodash.clonedeep": "4.5.0",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@serverless/typescript": "3.38.0",
    "@types/aws-lambda": "8.10.130",
    "@types/cors": "2.8.17",
    "@types/dateformat": "5.0.2",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.5",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/node": "20.10.5",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "dotenv": "16.3.1",
    "esbuild": "0.19.10",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.2",
    "serverless": "3.38.0",
    "serverless-esbuild": "1.50.1",
    "serverless-plugin-log-subscription": "2.2.0",
    "serverless-prune-plugin": "2.0.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "type-fest": "4.8.3",
    "typescript": "5.3.3"
  },
  "author": "Can Sahin",
  "license": "GPL-3"
}
