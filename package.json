{
  "name": "isa-users",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "lint": "eslint .",
    "prettifyAll": "prettier --write '{src,test}/**/*.ts'",
    "checkTs": "tsc --noEmit",
    "package": "serverless package",
    "deploy": "serverless deploy --verbose",
    "deploy:api": "serverless deploy function -f api",
    "deploy:cognito": "serverless deploy function -f cognitoTrigger",
    "deploy:verification-api": "serverless deploy function -f verificationApi",
    "deploy:public-api": "serverless deploy function -f publicApi",
    "scripts:create-user": "ts-node misc/scripts/create-user.ts",
    "scripts:listIncorrectItems": "ts-node misc/scripts/listIncorrectItems.ts",
    "scripts:remindUnverifiedUsers": "ts-node misc/scripts/remindUnverifiedUsers.ts"
  },
  "engines": {
    "node": ">=16.13.2"
  },
  "dependencies": {
    "@vendia/serverless-express": "4.8.2",
    "cors": "2.8.5",
    "dateformat": "5.0.3",
    "express": "4.18.1",
    "googleapis": "105.0.0",
    "jsonwebtoken": "8.5.1",
    "lodash.clonedeep": "4.5.0",
    "winston": "3.7.2"
  },
  "devDependencies": {
    "@serverless/typescript": "3.19.0",
    "@types/aws-lambda": "8.10.100",
    "@types/dateformat": "5.0.0",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.13",
    "@types/jsonwebtoken": "8.5.9",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/node": "16.0.0",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "dotenv": "16.3.1",
    "esbuild": "0.14.46",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "json-schema-to-ts": "2.5.3",
    "serverless": "3.19.0",
    "serverless-esbuild": "1.30.0",
    "serverless-plugin-log-subscription": "2.1.2",
    "serverless-prune-plugin": "2.0.1",
    "ts-node": "10.8.1",
    "tsconfig-paths": "4.0.0",
    "type-fest": "2.13.1",
    "typescript": "4.7.4"
  },
  "author": "Can Sahin",
  "license": "GPL-3"
}
